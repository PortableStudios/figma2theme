{
  "name": "@portablestudios/figma2theme",
  "version": "0.6.4",
  "author": "Darcy Glennen <darcy@portable.com.au>",
  "contributors": [
    "Darcy Glennen <darcy@portable.com.au>",
    "Cam Stark <cam@portable.com.au>"
  ],
  "license": "MIT",
  "main": "lib/src/index.js",
  "bin": {
    "figma2theme": "lib/src/index.js"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PortableStudios/figma2theme"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "concurrently --group yarn:typecheck yarn:test",
    "postinstall": "patch-package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "ts-node ./src/index.ts",
    "prebuild": "rimraf ./lib",
    "build": "tsc -p .",
    "postbuild": "rimraf ./lib/theme && copyfiles ./templates/** ./lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "verify": "concurrently --group yarn:test yarn:lint yarn:typecheck",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "global:install": "yarn build && yarn global add file:$PWD",
    "global:remove": "yarn global remove figma2theme"
  },
  "dependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash.setwith": "^4.3.6",
    "@types/prettier": "^2.4.3",
    "@types/svgo": "^2.6.1",
    "color-convert": "^2.0.1",
    "colors": "^1.4.0",
    "commander": "^8.3.0",
    "date-fns": "^2.28.0",
    "dotenv": "^11.0.0",
    "ejs": "^3.1.6",
    "figma-api": "1.7.0",
    "fs-extra": "^10.0.0",
    "lodash.setwith": "^4.3.2",
    "node-fetch": "^2.6.6",
    "polished": "^4.1.3",
    "prettier": "^2.5.1",
    "prompts": "^2.4.2",
    "svg-to-jsx": "^1.0.4",
    "svgo": "^2.8.0",
    "svgson": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@chakra-ui/react": "^1.7.4",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/react": "^6.4.12",
    "@storybook/theming": "^6.4.12",
    "@types/jest": "^27.4.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/prompts": "^2.0.14",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "framer-motion": "^5.5.7",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "lodash.merge": "^4.6.2",
    "patch-package": "^6.4.7",
    "pinst": "^2.1.6",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "storybook-addon-designs": "^6.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
