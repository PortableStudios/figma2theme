{
  "name": "@portablestudios/figma2theme",
  "version": "0.4.7",
  "author": "Darcy Glennen <darcy@portable.com.au>",
  "license": "MIT",
  "main": "lib/src/index.js",
  "bin": {
    "figma2theme": "lib/src/index.js"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PortableStudios/figma2theme"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "postinstall": "patch-package && husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "ts-node ./src/index.ts",
    "prebuild": "rimraf ./lib",
    "build": "tsc -p .",
    "postbuild": "rimraf ./lib/theme && copyfiles ./templates/** ./lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --fix --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "verify": "concurrently \"yarn:test\" \"yarn:lint\" \"yarn:typecheck\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "global:install": "yarn build && yarn global add file:$PWD",
    "global:remove": "yarn global remove figma2theme"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/ejs": "^3.0.5",
    "@types/fs-extra": "^9.0.7",
    "@types/lodash.setwith": "^4.3.6",
    "@types/prettier": "^2.2.0",
    "@types/rimraf": "^3.0.0",
    "@types/svgo": "^1.3.3",
    "color-convert": "^2.0.1",
    "colors": "^1.4.0",
    "commander": "^7.0.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "figma-api": "^1.5.5",
    "fs-extra": "^9.1.0",
    "lodash.setwith": "^4.3.2",
    "node-fetch": "^2.6.1",
    "polished": "^4.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "svg-to-jsx": "^1.0.2",
    "svgo": "^1.3.2",
    "svgson": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@chakra-ui/react": "^1.3.2",
    "@chakra-ui/theme-tools": "^1.0.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@storybook/theming": "^6.1.20",
    "@types/jest": "^26.0.20",
    "@types/lodash.merge": "^4.6.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "framer-motion": "^3.3.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash.merge": "^4.6.2",
    "patch-package": "^6.2.2",
    "pinst": "^2.1.4",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
